{
  "name": "sushi-chat-server",
  "version": "1.0.0",
  "description": "server of sushi chat",
  "main": "app.js",
  "scripts": {
    "start": "node --require dotenv/config ./dist/src/app.js",
    "build": "tsc -p .",
    "dev": "ts-node-dev --require dotenv/config --transpile-only --rs ./src/app.ts",
    "test": "jest --setupFiles dotenv/config",
    "test:watch": "jest --watch --setupFiles dotenv/config",
    "lint": "eslint ./src",
    "fix": "prettier --write \"**/*.{js,ts}\" --ignore-path .gitignore .",
    "fix:dry-run": "prettier --check \"**/*.{js,ts}\" --ignore-path .gitignore ."
  },
  "author": "Eri-0910",
  "license": "ISC",
  "dependencies": {
    "@socket.io/admin-ui": "^0.1.1",
    "@socket.io/redis-adapter": "^7.0.0",
    "@types/express": "^4.17.11",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.1",
    "graphemesplit": "^2.4.4",
    "node-uuid": "^1.4.8",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.0.1",
    "socket.io-mock": "^1.3.1",
    "sushi-chat-shared": "^1.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node-uuid": "^0.0.28",
    "@types/pg": "^7.14.11",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "jest": "^26.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
